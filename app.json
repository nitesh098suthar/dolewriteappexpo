{
  "expo": {
    "name": "Dolewrite",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dolewrite.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dolewrite.png",
        "resizeMode": "cover",
        "imageWidth": "100%",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nitesh098suthar.myapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/dolewrite.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/splashScreen.png",
          "dark": {
            "image": "./assets/images/splashScreen.png",
            "backgroundColor": "#ffffff",
            "resizeMode": "cover"
          },
          "resizeMode": "cover"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1eaf51aa-30e9-4e20-9e30-c757f538c8e4"
      }
    }
  }
}
